{{#each timelines}}

<div class="blogpost">

  <div class="sidebar">

    <div class="timestamp">
      <span class="tstamp">
        {{timestamp}}
      </span>

      <span class="pstamp">
        {{poststamp}}
      </span>
    </div>

    <div class="icon">
        <i class="fa {{icon}}"></i>
    </div>
  </div>

  <div class="post">

    <div class="title">
      <h1>{{title}}</h1>
    </div>

    <p class ="posttext">{{content}}</p>

  </div>
</div>
  {{/each}}
